{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-flow",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "emotion",
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    "@babel/plugin-transform-runtime",
    "babel-plugin-extract-react-types"
  ]
}
